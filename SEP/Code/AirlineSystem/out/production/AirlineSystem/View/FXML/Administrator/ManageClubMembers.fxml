<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="anchorPane" prefHeight="403.0" prefWidth="757.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ClubMembersController">
    <TableView fx:id="clubMembersTable" onMouseClicked="#removeButtonAppear" layoutX="194.0" prefHeight="400.0" prefWidth="561.0">
        <columns>
            <TableColumn fx:id="name" prefWidth="95.0" text="Name"/>
            <TableColumn fx:id="id" prefWidth="127.0" text="ID"/>
            <TableColumn fx:id="idType" prefWidth="127.0" text="ID Type"/>
            <TableColumn fx:id="nationality" prefWidth="127.0" text="Nationality"/>
            <TableColumn fx:id="birthday" prefWidth="83.0" text="Birthday"/>
            <TableColumn fx:id="phoneNumber" prefWidth="49.0" text="phone Number"/>
            <TableColumn fx:id="email" prefWidth="126.0" text="Email"/>
            <TableColumn fx:id="address" prefWidth="127.0" text="Address"/>
            <TableColumn fx:id="membershipDate" prefWidth="127.0" text="MembeshipDate"/>
        </columns>
    </TableView>
    <Button cancelButton="true" layoutX="14.0" layoutY="14.0" mnemonicParsing="false"
            onAction="#goBack" text="&lt;"/>
    <Button fx:id="removeButton" layoutX="14.0" layoutY="156.0" mnemonicParsing="false"
            onAction="#removeClubMemberButtonPressed" prefHeight="31.0" prefWidth="166.0" text="Remove member"
            visible="false"/>
    <TextField layoutX="14.0" layoutY="67.0" prefHeight="31.0" prefWidth="166.0" promptText="Search members"/>

</AnchorPane>
