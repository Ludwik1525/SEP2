<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.Administrator.ManageAirportsController" prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="airportsTable" layoutX="297.0" >
        <columns>
            <TableColumn fx:id="code"  text="Code"/>
            <TableColumn fx:id="Name"  text="Name"/>
            <TableColumn fx:id="City"  text="City"/>
            <TableColumn fx:id="zipCode"  text="Zip Code"/>
            <TableColumn fx:id="country"  text="Country"/>
            <TableColumn fx:id="numberOfGates"  text="Number Of Gates"/>
        </columns>
    </TableView>
    <Button cancelButton="true" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="&lt;"/>
    <Button layoutX="13.0" layoutY="67.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="262.0" onAction="#addButtonPressed"
            text="Add airport"/>
    <Button layoutX="14.0" layoutY="156.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="262.0" onAction="#removeButtonPressed"
            text="Remove airport"/>
    <TextField fx:id="searchAirports" layoutX="14.0" layoutY="112.0" prefHeight="31.0" prefWidth="262.0" promptText="Search airports"/>
</AnchorPane>
